{
  "name": "taranis-ng-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/compat": "^3.2.47",
    "axios": "^1.3.2",
    "js-base64": "^3.7.5",
    "papaparse": "^5.3.2",
    "vee-validate": "^2.2.15",
    "vue": "^3.2.47",
    "vue-chartjs": "4.1.2",
    "vue-cookies": "^1.8.1",
    "vue-datepicker-next": "^1.0.3",
    "vue-i18n": "^9.2.2",
    "vue-keypress": "^1.4.0",
    "vue-quick-chat": "^1.2.8",
    "vue-router": "^4.1.6",
    "vue-shortkey": "^3.1.7",
    "vue2-dropzone-vue3": "^1.1.0",
    "vue3-quill": "^0.3.0",
    "vuetify": "^3.1.11",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@babel/helper-compilation-targets": "^7.16.3",
    "@mdi/font": "^6.9.96",
    "@vue/cli": "^5.0.8",
    "@vue/cli-service": "~5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "material-design-icons-iconfont": "^6.1.1",
    "sass": "^1.59.2",
    "sass-loader": "^13.2.1",
    "webpack": "^5.76.3"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {
      "camelcase": "off",
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true
        }
      ],
      "space-before-function-paren": 0,
      "vue/multi-word-component-names": "off"
    },
    "root": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%"
  ]
}
