{
  "name": "taranis-ui",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run patch_config",
    "preview": "vite preview",
    "prepack": "node ./extras/update_version.js",
    "postpack": "node ./extras/remove_version.js",
    "lint": "eslint --ext .js,.vue --fix src",
    "package": "npm run prepack && npm pack && npm run postpack",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "lint_and_format": "npm run lint && npm run format",
    "patch_config": "node ./extras/patch_config_json.js",
    "test": "npm run lint && npm run format"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@mdi/font": "^7.3.67",
    "@sentry/vue": "^7.73.0",
    "@vueup/vue-quill": "^1.2.0",
    "axios": "^1.5.1",
    "js-base64": "^3.7.5",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4",
    "vue-chartjs": "5.2.0",
    "vue-datepicker-next": "^1.0.3",
    "vue-dompurify-html": "^4.1.4",
    "vue-i18n": "^9.5.0",
    "vue-keypress": "^2.1.2",
    "vue-pdf-embed": "1.2.1",
    "vue-router": "^4.2.5",
    "vue-shortkey": "^3.1.7",
    "vue2-dropzone-vue3": "^1.1.0",
    "vuetify": "^3.3.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.8.19"
  }
}
